{
    "$schema": "https://swc.rs/schema.json",
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": false,
        "decorators": true,
        "dynamicImport": true
      },
      "target": "es2020",
      "baseUrl": "./src",
      "paths": {
        "*": ["node_modules", "src/*"]
      }
    },
    "module": {
      "type": "commonjs"
    }
  }